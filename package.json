{
   "name": "selenium-mocha-digital-skola",
   "version": "1.0.0",
   "description": "Selenium webdriver test using mocha with Mochawesome report",
   "scripts": {
      "test": "mocha tests/specs --recursive --timeout 60000 --reporter mochawesome",
      "login": "mocha tests/specs/login.test.js --timeout 60000",
      "screenshot": "mocha tests/specs/login_ss.test.js --timeout 60000",
      "visual": "mocha tests/specs/visual.test.js --timeout 60000"
   },
   "dependencies": {
      "pixelmatch": "7.1.0",
      "pngjs": "7.0.0",
      "selenium-webdriver": "^4.40.0"
   },
   "devDependencies": {
      "@types/selenium-webdriver": "^4.35.5",
      "mocha": "^10.4.0",
      "mocha-simple-html-reporter": "2.0.0",
      "mochawesome": "^7.1.3"
   }
}
